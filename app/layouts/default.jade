    header.header
      .container-fluid
        .row
          h1.text-muted
            a(href='/')
              img.img-responsive(src='./images/SmartUp_Brand.svg', alt='smartup')
              span.sr-only smartup
          .nav-wrap.navbar
            button.navbar-toggle.collapsed.navbar-trigger(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
              span.sr-only Toggle navigation
              i.fa.fa-bars
            #bs-example-navbar-collapse-1.collapse.navbar-collapse
              .login-box
                p YOUR COMMUNITY
                .login-btns
                  a.cbtn.btn-dark(href='#0') create
                  a.cbtn.btn-bright.borderless(href='#0') sign in / find
              nav.main-nav
                ul.nav.pull-right
                  li.active
                    a(href='features.html') Features
                  li
                    a(href='pricing.html') Pricing
                  li
                    a(href='#') Blog
    

    block content

    
    footer.footer
      .container
        .row
          .col-xs-6
            small.copy
              span SmartUp.io, LTD &copy; 2017
              span
                a(href='privacy.html') Privacy
              span
                a(href='copyright.html') Copyright
              span
                a(href='terms_and_services.html') Terms of Service
              span
                a(href='#0') Teams
              span
                a(href='contact.html') Contact
          .col-xs-6
            ul.social
              li
                a(href='#0')
                  i.fa.fa-facebook
              li
                a(href='#0')
                  i.fa.fa-twitter
              li
                a(href='#0')
                  i.fa.fa-instagram
              li
                a(href='#0')
